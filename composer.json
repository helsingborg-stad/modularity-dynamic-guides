{
    "name": "helsingborg/modularity-dynamic-guides",
    "description": "A plugin to create dynamic guides",
    "type": "wordpress-plugin",
    "license": "MIT",
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox",
        "coverage": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --testdox",
        "minimal": "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Niclas Norin",
            "email": "niclas.norin@helsingborg.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "ModularityDynamicGuides\\": "source/php/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "10up/wp_mock": "^1.0",
        "lucatume/function-mocker": "^1.3",
        "helsingborg-stad/phpcs": "^0.3.5"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "version": "1.3.0"
}